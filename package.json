{
  "name": "react-dms-input",
  "version": "0.0.5",
  "license": "MIT",
  "homepage": "https://phalkone.github.io/react-dms-input/",
  "repository": {
    "url": "git+https://github.com/phalkone/react-dms-input.git"
  },
  "keywords": [
